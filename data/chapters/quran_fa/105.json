  "result": [
    {
      "id": "6189",
      "sura": "105",
      "aya": "1",
      "arabic_text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِأَصۡحَٰبِ ٱلۡفِيلِ",
      "translation": "[ای پیامبر،] آیا ندیدی پروردگارت با اصحاب فیل چه کرد؟",
      "footnotes": ""
    },
    {
      "id": "6190",
      "sura": "105",
      "aya": "2",
      "arabic_text": "أَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِي تَضۡلِيلٖ",
      "translation": "آیا [فریب و] نقشۀ آنان را در تباهی قرار نداد؟",
      "footnotes": ""
    },
    {
      "id": "6191",
      "sura": "105",
      "aya": "3",
      "arabic_text": "وَأَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا أَبَابِيلَ",
      "translation": "و پرندگانی گروه‌گروه بر [سر] آنان فرستاد",
      "footnotes": ""
    },
    {
      "id": "6192",
      "sura": "105",
      "aya": "4",
      "arabic_text": "تَرۡمِيهِم بِحِجَارَةٖ مِّن سِجِّيلٖ",
      "translation": "[که] با سنگ‌گِل [که در منقار و چنگال داشتند] آنان را سنگباران می‌کردند؛",
      "footnotes": ""
    },
    {
      "id": "6193",
      "sura": "105",
      "aya": "5",
      "arabic_text": "فَجَعَلَهُمۡ كَعَصۡفٖ مَّأۡكُولِۭ",
      "translation": "و آنها را همچون کاهِ جویده‌شده ساخت.",
      "footnotes": ""
    }
  ]
}
